
.overlay.data-v-5c61d442 {
		position: fixed;
		top: 0;
		left: 0;
		right: 0;
		bottom: 0;
		z-index: 999;
		display: flex;
		justify-content: center;
		align-items: flex-end;
		background-color: rgba(0, 0, 0, 0.5);
		/* 背景遮罩 */
}
.mask.data-v-5c61d442 {
		position: absolute;
		top: 0;
		left: 0;
		right: 0;
		bottom: 0;
		z-index: 998;
}
.picker-wrapper.data-v-5c61d442 {
		width: 100%;
		background-color: #fff;
		border-top-left-radius: 10px;
		border-top-right-radius: 10px;
		z-index: 999;
		position: relative;
		transition: transform 0.3s ease-out;
		transform: translateY(100%); /* 初始位置在屏幕外 */
}
.picker-wrapper-show.data-v-5c61d442 {
		transform: translateY(0); /* 显示时从下方滑入 */
}
.picker-wrapper-hide.data-v-5c61d442 {
		transform: translateY(100%); /* 隐藏时滑出屏幕 */
}
.picker-header.data-v-5c61d442 {
		display: flex;
		justify-content: space-between;
		padding: 15px 20px;
		font-size: 18px;
		background-color: #f8f8f8;
		border-bottom: 1px solid #ddd;
}
.cancel.data-v-5c61d442{
		font-size: 18px;
		color: #434343;
}
.confirm.data-v-5c61d442 {
		font-size: 18px;
		color: #9ca1d3;
}
.picker-view.data-v-5c61d442 {
		width: 100%;
		height: 200px;
}
.picker-item.data-v-5c61d442 {
		height: 50px;
		line-height: 50px;
		text-align: center;
		font-size: 16px;
}
